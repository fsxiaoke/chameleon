<script cml-type="interface">
interface OnlyInterfaceInterface {
  getMsg(msg: String): String;
  setTitle(title: String): void;
}

</script>

<script cml-type="web">

class Method implements OnlyInterfaceInterface {
  getMsg(msg) {
    return 'web:' + msg;
  }
  setTitle(title) {
    return true;
  }
}

export default new Method();
</script>

